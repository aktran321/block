<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <img src="/assets/img/block-logos-03.svg">
  </div>
</div>

<div class="container-fluid user-input-container">
  <form id="signup-form">
    <div class="form-group">
      <label for="new-email">Email address</label>
      <input type="email" class="form-control" id="new-email" aria-describedby="emailHelp" placeholder="Enter email">
      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
    </div>
    <div class="form-group">
      <label for="new-password">Password</label>
      <input type="password" class="form-control" id="new-password" placeholder="Password">
    </div>
    <a href="classInput">
      <button type="submit" class="btn btn-secondary" id="sign-up-button">Submit</button>
    </a>
  </form>
</div>

<div class="container-fluid display-schedule-container">
  <div class="timetable-display">
      <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner">
          <div class="carousel-item">
            {{!-- {{timetable-element}} --}}
          </div>
          <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
    </div>
    <button type="save" id="save-button" class="btn btn-secondary">Save</button>
</div>

<footer class="card-footer text-muted">
